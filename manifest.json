{
  "name": "async react toolbox",
  "short_name": "Async React",
  "version": "0.1",
  "description":
    "Open source tools to debug your async react apps. Click on the icon to see the lag radar, Ctrl+R to show/hide",
  "permissions": ["activeTab", "declarativeContent", "tabs", "http://*/*", "https://*/*"],
  "browser_action": {
    "default_title": "Async React Toolbox"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "options.html",
  "homepage_url": "https://github.com/sw-yx/async-react-toolbox",
  "incognito": "spanning",
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "{c6988e4e-86ea-4866-ad75-700bddaefc66}"
    }
  }
}
